{
  "buildCommand": "mkdir -p .vercel/output/static && cp index.html .vercel/output/static/index.html",
  "outputDirectory": ".vercel/output/static",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
